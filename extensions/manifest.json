{
  "manifest_version": 2,
  "name": "minamo.cloud Terminal Extension",
  "version": "0.0.5",
  "author": "minamo.cloud development team",
  "background": {
    "scripts": [ "main.js" ],
    "persistent": false
  },
  "content_scripts": [{
    "matches": [ "https://*/*", "http://*/*" ],
    "js": [ "content_script.js" ],
    "run_at": "document_end"
  }],
  "browser_action": {
  },
  "permissions": [
    "tabs", "storage"
  ],
  "options_page": "options.html"
}
