{
  "manifest_version": 2,
  "name": "minamo.io Terminal Extension",
  "version": "0.0.4",
  "author": "minamo.io development team",
  "background": {
    "scripts": [ "main.js" ],
    "persistent": false
  },
  "content_scripts": [{
    "matches": [ "https://minamo.io/*" ],
    "js": [ "content_script.js" ]
  }],
  "browser_action": {
  },
  "permissions": [
    "tabs"
  ]
}
